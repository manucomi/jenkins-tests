        "react-dom": "18.2.0",
        "release-it": "15.5.1",
        "rollup": "3.8.0",
        "rollup-plugin-copy": "3.4.0",
        "rollup-plugin-postcss": "4.0.2",
        "sass": "1.56.2",
        "sass-loader": "13.2.0",
        "style-loader": "3.3.1",
        "stylelint": "14.16.0",
        "stylelint-config-prettier": "9.0.4",
        "stylelint-config-sass-guidelines": "9.0.1",
        "stylelint-prettier": "2.0.0",
        "yalc": "1.0.0-pre.53"
    },
    "peerDependencies": {
        "react": "^18.0.0",
        "react-dom": "^18.0.0"
    },
    "publishConfig": {
