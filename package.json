{
    "name": "mfe-core",
    "version": "0.0.20",
    "description": "React component library for HBRG micro front-ends",
    "scripts": {
        "build": "npm run clean:build && rollup -c --bundleConfigAsCjs  && cp -a package.json dist/ && cd dist/ && npm pkg delete scripts.prepare",
        "clean": "rm -rf .tmp/ && npm run clean:build",
        "clean:all": "npm run clean && rm -rf node_modules/",
        "clean:build": "rm -rf dist/",
        "lint": "npm run lint:js && npm run lint:scss",
        "lint:fix": "npm run lint:js:fix && npm run lint:scss:fix",
        "lint:js": "eslint --ext .js,.jsx,.mdx .",
        "lint:js:fix": "npm run lint:js -- --fix",
        "lint:scss": "stylelint **/*.{css,scss}",
        "lint:scss:fix": "npm run lint:scss -- --fix",
        "prepare": "husky install",
        "release": "npm run build && npm publish dist/",
        "release:local": "npm run build && yalc push dist/",
        "release:notes": "release-it --no-npm --no-git --no-increment --github.release --ci",
        "sb": "start-storybook --port 6006 --config-dir storybook/",
        "sb:a11y": "test-storybook --config-dir storybook/ --maxWorkers=2",
        "sb:build": "build-storybook --config-dir storybook/ --output-dir .tmp/storybook/",
        "sb:test": "jest --config jest.visual.config.js",
        "test": "npm run unit:test && npm run sb:test",
        "unit:test": "jest"
    },
    "dependencies": {
        "@babel/runtime": "7.20.6",
        "classnames": "2.3.2",
        "cookie-cutter": "0.2.0",
        "core-js": "3.26.1",
        "focus-trap-react": "10.0.2",
        "lodash": "4.17.21",
        "mobile-detect": "1.4.5",
        "prop-types": "15.8.1",
        "react-google-recaptcha-enterprise": "1.0.3",
        "react-tabs": "6.0.0",
        "swr": "2.0.0",
        "zustand": "4.1.5"
    },
    "devDependencies": {
        "@babel/core": "7.20.5",
        "@babel/eslint-parser": "7.19.1",
        "@babel/plugin-transform-runtime": "7.19.6",
        "@babel/preset-env": "7.20.2",
        "@babel/preset-react": "7.18.6",
        "@rollup/plugin-alias": "4.0.2",
        "@rollup/plugin-babel": "6.0.3",
        "@rollup/plugin-commonjs": "23.0.4",
        "@rollup/plugin-node-resolve": "15.0.1",
        "@rollup/plugin-terser": "0.2.1",
        "@rollup/plugin-url": "8.0.1",
        "@storybook/addon-a11y": "6.5.14",
        "@storybook/addon-actions": "6.5.14",
        "@storybook/addon-docs": "6.5.14",
        "@storybook/addon-essentials": "6.5.14",
        "@storybook/addon-interactions": "6.5.14",
        "@storybook/addon-links": "6.5.14",
        "@storybook/addon-storyshots": "6.5.14",
        "@storybook/addon-storyshots-puppeteer": "6.5.14",
        "@storybook/builder-webpack5": "6.5.14",
        "@storybook/manager-webpack5": "6.5.14",
        "@storybook/preset-scss": "1.0.3",
        "@storybook/react": "6.5.14",
        "@storybook/test-runner": "0.9.1",
        "@storybook/testing-library": "0.0.13",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "13.4.0",
        "@testing-library/user-event": "14.4.3",
        "axe-playwright": "1.1.11",
        "babel-jest": "29.3.1",
        "babel-loader": "9.1.0",
        "css-loader": "6.7.2",
        "eslint": "8.29.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-prettier": "8.5.0",
        "eslint-import-resolver-alias": "1.1.2",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jest-dom": "4.0.3",
        "eslint-plugin-jsx-a11y": "6.6.1",
        "eslint-plugin-mdx": "1.17.1",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.31.11",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-storybook": "0.6.8",
        "eslint-plugin-testing-library": "5.9.1",
        "husky": "8.0.2",
        "identity-obj-proxy": "3.0.0",
        "jest": "29.3.1",
