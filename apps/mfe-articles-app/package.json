{
    "name": "mfe-articles-app",
    "version": "0.0.1",
    "scripts": {
        "build": "next build",
        "clean": "npm run clean:build && npm run clean:deps && npm run clean:test",
        "clean:build": "rm -rf .next/",
        "clean:deps": "rm -rf node_modules/",
        "clean:test": "rm -rf .coverage/",
        "dev": "node server",
        "dns-alias": "hostile set localhost mfe.dev.hbr.org",
        "link:mfe-core": "yalc add mfe-core && yalc link mfe-core && npm install",
        "link:check": "yalc check",
        "lint": "npm run lint:js && npm run lint:scss",
        "lint:fix": "npm run lint:js:fix && npm run lint:scss:fix",
        "lint:js": "eslint --ext .js,.jsx .",
        "lint:js:fix": "eslint --fix --ext .js,.jsx .",
        "lint:scss": "stylelint -f verbose '**/*.{css,scss}'",
        "lint:scss:fix": "npm run lint:scss -- --fix",
        "prettier": "npx prettier --write .",
        "release:notes": "release-it --no-npm --no-git --no-increment --github.release --ci",
        "setup": "npm run dns-alias",
        "start": "NODE_OPTIONS='-r @newrelic/next' next start",
        "test": "jest",
        "sb": "storybook dev -p 6006 --config-dir storybook/",
        "sb:build": "storybook build --config-dir storybook/ --output-dir .tmp/storybook/",
        "sb:chromatic": "npx chromatic -b sb:build --project-token=${CHROMATIC_TOKEN} --ci --only-changed",
        "unlink:mfe-core": "yalc remove mfe-core && rm package-lock.json && npm install"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@newrelic/next": "0.7.0",
        "classnames": "2.3.2",
        "eslint-import-resolver-alias": "1.1.2",
        "html-react-parser": "5.0.7",
        "mfe-articles-renderer": "*",
        "mfe-articles-shared": "*",
        "mfe-core": "1.5.1-pr.687c2bc",
        "next": "14.0.4",
        "prop-types": "15.8.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "zustand": "4.4.7"
    },
    "devDependencies": {
        "@storybook/addon-a11y": "7.6.3",
        "@storybook/addon-essentials": "7.6.3",
        "@storybook/addon-interactions": "7.6.3",
        "@storybook/addon-links": "7.6.3",
        "@storybook/blocks": "7.6.3",
        "@storybook/nextjs": "7.6.3",
        "@storybook/react": "7.6.3",
        "@storybook/testing-library": "0.2.2",
        "@testing-library/jest-dom": "6.1.5",
        "@testing-library/react": "14.1.2",
        "@testing-library/user-event": "14.5.1",
        "chromatic": "10.1.0",
        "compression": "1.7.4",
        "eslint": "8.55.0",
        "eslint-config-mfe-articles": "*",
        "express": "4.18.2",
        "hostile": "1.3.3",
        "http-proxy-middleware": "2.0.6",
        "husky": "8.0.3",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "msw": "1.3.2",
        "msw-storybook-addon": "1.10.0",
        "postcss": "8.4.32",
        "postcss-flexbugs-fixes": "5.0.2",
        "postcss-preset-env": "9.3.0",
        "postcss-pxtorem": "6.0.0",
        "postcss-scss": "4.0.9",
        "prettier": "3.1.0",
        "sass": "1.69.5",
        "storybook": "7.6.3",
        "stylelint": "15.11.0",
        "stylelint-config-sass-guidelines": "10.0.0",
        "stylelint-order": "6.0.3",
        "stylelint-prettier": "4.1.0",
        "yalc": "1.0.0-pre.53"
    },
    "msw": {
        "workerDirectory": "storybook/public"
    }
}