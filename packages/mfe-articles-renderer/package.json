{
  "name": "mfe-articles-renderer",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "build": "npm run clean:build && rollup -c --bundleConfigAsCjs && npm run build:post",
    "build:post": "cp -a package.json dist/ && cd dist/ && npm pkg delete scripts.prepare",
    "clean": "rm -rf .tmp/ && npm run clean:build",
    "clean:all": "npm run clean && rm -rf node_modules/",
    "clean:build": "rm -rf dist/",
    "lint": "npm run lint:js",
    "lint:fix": "npm run lint:js:fix && npm run lint:scss:fix",
    "lint:js": "eslint --ext .js,.jsx,.mdx .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:scss": "stylelint **/*.{css,scss}",
    "lint:scss:fix": "npm run lint:scss -- --fix",
    "release": "npm run build && npm publish dist/",
    "release:local": "npm run build && yalc push dist/",
    "release:notes": "release-it --ci",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "7.23.6",
    "core-js": "3.34.0",
    "html-react-parser": "5.1.0",
    "mfe-articles-shared": "*",
    "prop-types": "15.8.1",
    "zustand": "4.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.6",
    "@babel/eslint-parser": "7.23.3",
    "@babel/plugin-transform-runtime": "7.23.6",
    "@babel/preset-env": "7.23.6",
    "@babel/preset-react": "7.23.3",
    "@rollup/plugin-alias": "5.1.0",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-url": "8.0.2",
    "@testing-library/jest-dom": "6.2.0",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.2",
    "babel-jest": "29.7.0",
    "babel-loader": "9.1.3",
    "css-loader": "6.8.1",
    "eslint": "8.55.0",
    "eslint-config-mfe-articles": "*",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "postcss-preset-env": "9.3.0",
    "postcss-pxtorem": "6.0.0",
    "rollup": "4.9.1",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-postcss": "4.0.2",
    "style-loader": "3.3.3",
    "yalc": "1.0.0-pre.53"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "publishConfig": {
    "registry": "http://nexus.hbsp.harvard.edu/content/repositories/npm-internal/"
  }
